<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Administrator</title>
	<link rel="stylesheet" href="css/main.css">
</head>
<body>
	<div class="header"></div>
	<div class="overlay"></div>
	<form class="departmentAdd row">
		<div class="departmentAdd_curYear">
			<label for="">Year</label>
			<input type="text">
		</div>
		<div class="departmentAdd_table">
			<div class="btn--close">X</div>
			<h3>Department<span href="" class="content_title_btn">Add</span></h3>
			<table>
				<thead>
					<tr>
						<th>
							<label for="">Department Name</label>
							<input type="text">
						</th>
						<th>
							<label for="">Department Description</label>
							<input type="text">	
						</th>
					</tr>
				</thead>
				<tbody class="departmentAdd_courses">
					<tr>
						<th>Course Code</th>
						<th>Course Description</th>
						<th># of years</th>
						<th># of terms</th>
						<th>add</th>
						<th>delete</th>
					</tr>
					<tr>
						<td>
							<input type="text">
						</td>
						<td>
							<input type="text">
						</td>
						<td>
							<input type="text">
						</td>
						<td>
							<input type="text">
						</td>
					</tr>
				</tbody>
			</table>
			<div class="btn">Next</div>
		</div>
	</form>
	<div class="container admin">
		<div class="three column admin_sidebar">
			<div class="person">
				<div class="person_image"><img src="img/me.png" alt=" Profile picture "></div>
				<div class="person_name">Robert Admin</div>
				<div class="person_role">Administrator</div>
			</div>
			<div class="admin_sidebarNav">
				<ul>
					<li class="admin_sidebarNav--list">
						<a class="admin_sidebarNav--item">Curriculum</a>
					</li>
					<li class="admin_sidebarNav--list">
						<a class="admin_sidebarNav--item">Assestments</a>
					</li>
					<li class="admin_sidebarNav--list">
						<a class="admin_sidebarNav--item">Reports</a>
					</li>
					<li class="admin_sidebarNav--list">
						<a class="admin_sidebarNav--item">Blog</a>
					</li>
				</ul>
			</div>
		</div>
		<div class="nine column admin_content">
			<article class="content row">
				<div class="row">
					<h2 class="content_title">Curriculum<span href="" class="departmentAdd--show content_title_btn">Add</span></h2>
				</div>
				<div class="curriculum four column">
					<div class="box cf">
						<div class="tag"><h3>2015</h3></div>
						<div class="tag">Curriculum</div>
						<div class="box--overlay">
							<div class="box_btn box_btn--blue"><a href="/curriculum">View</a></div>
							<div class="box_btn box_btn--orange"><a href="">Edit</a></div>
							<div class="box_btn box_btn--red"><a href="">Delete</a></div>
						</div>
					</div>
				</div>
				<div class="curriculum four column">
					<div class="box cf">
						<div class="tag"><h3>2014</h3></div>
						<div class="tag">Curriculum</div>
						<div class="box--overlay">
							<div class="box_btn box_btn--blue"><a href="/curriculum">View</a></div>
							<div class="box_btn box_btn--orange"><a href="">Edit</a></div>
							<div class="box_btn box_btn--red"><a href="">Delete</a></div>
						</div>
					</div>
				</div>
				<div class="curriculum four column">
					<div class="box cf">
						<div class="tag"><h3>2013</h3></div>
						<div class="tag">Curriculum</div>
						<div class="box--overlay">
							<div class="box_btn box_btn--blue"><a href="/curriculum">View</a></div>
							<div class="box_btn box_btn--orange"><a href="">Edit</a></div>
							<div class="box_btn box_btn--red"><a href="">Delete</a></div>
						</div>
					</div>
				</div>
			</article>
			<article class="content row">
				<div class="row">
					<h2 class="content_title">Assestment<span href="" class="content_title_btn">Add</span></h2>
				</div>
				<div class="assestment">
					<form action="" class="assestment_form">
						<div class="types_fee">
							<label for="fee_label" class="types_fee_label">Type of School Fee</label>
							<input type="text" id="fee_label">
						</div>
						<div class="assestment_mtn row">
							<table class=" twelve column">
								<thead>
									<tr>
										<th>
											<label for="fee_name">Fee name</label>
											<input type="text">
										</th>
										<th>
											<label for="fee_amount">Fee amount</label>
											<input type="text">
										</th>
										<th>
											<button class="btn btn--blue">Add</button>
										</th>
										<th>
											<button class="btn btn--green">Update</button>
										</th>
									</tr>
								</thead>
								<tbody class="assestment_mtn_body">
									<tr>
										<th>Name</th>
										<th>Amount</th>
									</tr>
									<tr>
										<td>Equipment</td>
										<td><span>P</span>1,000</td>
										<td><button class="btn btn--orange">Edit</button></td>
										<td><button class="btn btn--red">Delete</button></td>
									</tr>
									<tr>
										<td>Libary</td>
										<td><span>P</span>2,000</td>
										<td><button class="btn btn--orange">Edit</button></td>
										<td><button class="btn btn--red">Delete</button></td>
									</tr>
									<tr>
										<td>Insurance</td>
										<td><span>P</span>3,000</td>
										<td><button class="btn btn--orange">Edit</button></td>
										<td><button class="btn btn--red">Delete</button></td>
									</tr>
								</tbody>
							</table>
						</div>
					</form>
					<table class="assestment_fees twelve column">
						<thead>
							<tr>
								<th>Type of Fee</th>
								<th>Date Created</th>
								<th># of Fees</th>
								<th>Amount</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>Equipment</td>
								<td>1-2-3</td>
								<td>3</td>
								<td><span>P</span>10,000</td>
								<td><button class="btn btn--blue assestment_formView">View</button></td>
							</tr>
							<tr>
								<td>Libary</td>
								<td>6-4-2</td>
								<td>4</td>
								<td><span>P</span>8,000</td>
								<td><button class="btn btn--blue assestment_formView">View</button></td>
							</tr>
							<tr>
								<td>Insurance</td>
								<td>5-6-7</td>
								<td>6</td>
								<td><span>P</span>8,000</td>
								<td><button class="btn btn--blue assestment_formView">View</button></td>
							</tr>
						</tbody>
					</table>
				</div>
			</article>
		</div>
	</div>
<script src="lib/angular.min.js"></script>
<script src="lib/jquery-1.11.1.min.js"></script>
<script src="js/sideBarFixed.js"></script>
<script src="js/admin.js"></script>
</body>
</html>